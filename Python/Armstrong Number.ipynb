   "id": "010f3a4f-0564-4079-9ec9-b59fd2af5852",
   "metadata": {},
   "outputs": [
    {
     "name": "stdin",
     "output_type": "stream",
     "text": [
      "Enter the Number:  153\n"
     ]
    },
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Armstrong\n"
     ]
    }
   ],
   "source": [
    "n = int(input(\"Enter the Number: \"))\n",
    "original = n\n",
    "total = 0\n",
    "length = len(str(n))\n",
    "while(n>0):\n",
    "    digit = n%10\n",
    "    total += digit ** length\n",
    "    n //= 10\n",
    "\n",
    "if(original == total):\n",
    "    print(\"Armstrong\")\n",
    "else:\n",
    "    print(\"Not Armstrong\")"
   ]
